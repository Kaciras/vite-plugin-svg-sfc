{
	"name": "vite-plugin-svg-sfc",
	"version": "0.1.0",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"type": "module",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^2.8.0"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.3.0",
		"@kaciras/eslint-config-typescript": "^2.3.0",
		"@kaciras/eslint-config-vue": "^2.3.0",
		"@types/node": "^17.0.21",
		"@types/svgo": "^2.6.2",
		"@vitejs/plugin-vue": "^2.2.2",
		"@vue/server-renderer": "^3.2.29",
		"c8": "^7.11.0",
		"eslint": "^8.9.0",
		"rollup": "^2.68.0",
		"typescript": "^4.5.5",
		"vite": "^2.8.4",
		"vitest": "^0.5.4",
		"vue": "^3.2.31"
	}
}
