{
	"name": "vite-plugin-svg-sfc",
	"version": "2.0.1",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"type": "module",
	"exports": {
		"./client": {
			"types": "./client.d.ts"
		},
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"module": "index.js",
	"main": "index.cjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc --project tsconfig.lib.json && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^3.2.0"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^3.0.1",
		"@kaciras/eslint-config-typescript": "^3.0.1",
		"@kaciras/eslint-config-vue": "^3.0.1",
		"@types/node": "^20.12.7",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vitest/coverage-v8": "^1.5.0",
		"eslint": "^9.1.0",
		"rollup": "^4.16.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.10",
		"vitest": "^1.5.0",
		"vue": "^3.4.23"
	}
}
