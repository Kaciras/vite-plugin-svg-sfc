{
	"name": "vite-plugin-svg-sfc",
	"version": "1.2.0",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"module": "index.js",
	"main": "index.cjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^3.0.2"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.5.0",
		"@kaciras/eslint-config-typescript": "^2.5.0",
		"@kaciras/eslint-config-vue": "^2.5.0",
		"@types/node": "^18.14.6",
		"@types/ws": "^8.5.4",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitest/coverage-c8": "^0.29.2",
		"c8": "^7.13.0",
		"eslint": "^8.35.0",
		"rollup": "^3.18.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vitest": "^0.29.2",
		"vue": "^3.2.47",
		"ws": "^8.12.1"
	}
}
