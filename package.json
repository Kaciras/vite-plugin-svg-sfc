{
	"name": "vite-plugin-svg-sfc",
	"version": "2.0.1",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"type": "module",
	"exports": {
		"./client": {
			"types": "./client.d.ts"
		},
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"module": "index.js",
	"main": "index.cjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc --project tsconfig.lib.json && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^3.2.0"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.6.3",
		"@kaciras/eslint-config-typescript": "^2.6.3",
		"@kaciras/eslint-config-vue": "^2.6.3",
		"@types/node": "^20.11.13",
		"@types/ws": "^8.5.10",
		"@vitejs/plugin-vue": "^5.0.3",
		"@vitest/coverage-v8": "^1.2.2",
		"eslint": "^8.56.0",
		"rollup": "^4.9.6",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.2",
		"vue": "^3.4.15",
		"ws": "^8.16.0"
	}
}
