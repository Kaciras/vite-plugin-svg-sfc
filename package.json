{
	"name": "vite-plugin-svg-sfc",
	"version": "1.2.1",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"type": "module",
	"exports": {
		"./client": {
			"types": "./client.d.ts"
		},
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"module": "index.js",
	"main": "index.cjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc --project tsconfig.lib.json && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^3.0.2"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.6.1",
		"@kaciras/eslint-config-typescript": "^2.6.2",
		"@kaciras/eslint-config-vue": "^2.6.1",
		"@types/node": "^20.8.4",
		"@types/ws": "^8.5.6",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vitest/coverage-v8": "^0.34.6",
		"eslint": "^8.51.0",
		"rollup": "^4.0.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11",
		"vitest": "^0.34.6",
		"vue": "^3.3.4",
		"ws": "^8.14.2"
	}
}
