{
	"name": "vite-plugin-svg-sfc",
	"version": "0.4.0",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"type": "module",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^2.8.0"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.4.1",
		"@kaciras/eslint-config-typescript": "^2.4.1",
		"@kaciras/eslint-config-vue": "^2.4.1",
		"@types/node": "^18.7.16",
		"@types/svgo": "^2.6.4",
		"@types/ws": "^8.5.3",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vue/server-renderer": "^3.2.39",
		"c8": "^7.12.0",
		"eslint": "^8.23.0",
		"rollup": "^2.79.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.0",
		"vitest": "^0.23.2",
		"vue": "^3.2.39",
		"ws": "^8.8.1"
	}
}
