{
	"name": "vite-plugin-svg-sfc",
	"version": "1.1.0",
	"description": "Convert SVGs to Vue single file component(SFC), support <style> tag",
	"author": "Kaciras <Kaciras@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Kaciras/vite-plugin-svg-sfc"
	},
	"keywords": [
		"vite-plugin",
		"rollup-plugin",
		"vue",
		"svg"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"module": "index.js",
	"main": "index.cjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.cjs",
		"index.js",
		"client.d.ts"
	],
	"scripts": {
		"compile": "tsc && node scripts/build-cjs.js",
		"test": "vitest run"
	},
	"dependencies": {
		"svgo": "^3.0.2"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^2.5.0",
		"@kaciras/eslint-config-typescript": "^2.5.0",
		"@kaciras/eslint-config-vue": "^2.5.0",
		"@types/node": "^18.11.10",
		"@types/ws": "^8.5.3",
		"@vitejs/plugin-vue": "^3.2.0",
		"@vitest/coverage-c8": "^0.25.3",
		"c8": "^7.12.0",
		"eslint": "^8.29.0",
		"rollup": "^2.79.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vitest": "^0.25.3",
		"vue": "^3.2.45",
		"ws": "^8.11.0"
	}
}
