{
	"include": ["index.ts"],
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true,
		"useUnknownInCatchVariables": false,
		"allowSyntheticDefaultImports": true
	}
}
